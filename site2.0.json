    {
        "version": 1,
        "siteName": "69书吧",
        "baseUrl": "https://www.69shuba.com/",
        "author": "steve",
        "contact": "",
        "engine": "xpath",
        "enable": true,
        "header": {},
        "cookies": {},
        "ruleSearch": {
            "url": "https://www.69shuba.com/modules/article/search.php",
            "method": "POST",
            "params": {
                "searchkey": "{keyword}",
                "searchtype": "all"
            },
            "encode": "gbk",
            "bookList": "//*[@class='newbox']/ul/li",
            "bookName": ".//div[@class='newnav']/h3/a[2]",
            "bookUrl": ".//div[@class='newnav']/h3/a[1]/@href",
            "bookAuthor": ".//div[@class='labelbox']/label[1]/text()",
            "ruleExtra": {
                "imageUrl": ".//a/img/@src",
                "lastUpdateTime": "",
                "lastChapterName": "",
                "introduce": ".//ol[@class='ellipsis_2']/text()",
                "classify": ".//div[@class='labelbox']/label[2]/text()",
                "status": ".//div[@class='labelbox']/label[3]/text()"
            }
        },
        "ruleChapter": {
            "list": "//*[@id='catalog']/ul/li/a",
            "name": ".//text()",
            "url": ".//@href",
            "encode": "gbk",
            "event": "let reslut = result.replace('.htm', '/'); return reslut;"
        },
        "ruleContent": {
            "lines": "//*[@class='txtnav']/text()",
            "encode": "gbk",
            "cleaner": ""
        }
    },
